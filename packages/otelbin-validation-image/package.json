		"@opentelemetry/instrumentation-net": "^0.35.0",
		"@opentelemetry/resource-detector-aws": "^1.3.4",
		"@opentelemetry/resources": "^1.18.1",
		"@opentelemetry/sdk-metrics": "^1.18.1",
