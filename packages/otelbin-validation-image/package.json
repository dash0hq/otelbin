		"build:wrapper": "esbuild src/lambda-wrapper.ts --bundle --sourcemap --platform=node --target=es2020 --outfile=dist/lambda-wrapper.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dash0hq/otelbin.git"
	},
