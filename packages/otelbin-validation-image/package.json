	"bugs": {
		"url": "https://github.com/dash0hq/otelbin/issues"
	},
	"homepage": "https://github.com/dash0hq/otelbin#readme",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/aws-lambda": "^8.10.129",
		"@types/jest": "^29.5.12",
		"@types/js-yaml": "^4.0.8",
		"@types/node": "^20.8.10",
		"esbuild": "^0.19.8",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.2"
	},
	"dependencies": {
		"@expo/spawn-async": "^1.7.2",
		"aws-lambda": "^1.0.7",
